fni1 = \a, b -> if (a > b) return "up"; else return "down";;
sys.test(fni1(3, 2), "up");
sys.test(fni1("a", "z"), "down");

fni2 = \a, b -> {
  if (a > b) {
    return "up";
  } else if (a == b) {
    return "eq";
  } else {
    return "down";
  }
};
sys.test(fni2(3.5, 2.03), "up");
sys.test(fni2("a", "z"), "down");
sys.test(fni2(1, 1), "eq");

