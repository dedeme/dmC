fn1 = \(D|s) M -> return M.a;;
fn2 = \(Ds|s) M, tx ->  {
  return M.a + "4" + tx;
};
fn3 = \(Dss|s) M, tx1, tx2 -> {
  v1 = tx1 + tx2;
  return M.a + v1;
};
