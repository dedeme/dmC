/// Int tests

// EQUALITY
// ========
// Ints
128 128 == assert
12 128 != assert

// Floats
3.15 3.15 == assert
3.15 3.14 != assert

// Strings
"" "" == assert
"" "a" != assert
"cd" "" != assert
"abc" "abc" == assert
"abc" "acc" != assert

// Lists
() () == assert
(1) () != assert
() ("a") != assert
(1 2 x25) (1 2 x25) == assert
(1 2 x25) (1 2 x24) != assert
(1 2 x25) (1 2 x25 3) != assert
(1 ("a" 3) +) (1 ("a" 3) +) == assert
{"one": 1, "two": 2} ("one" 1 "two" 2) == assert

// LOGIC OPERATORS
// ===============
(&& ==) andEq =>
// And
1 1 123 andEq assert
0 0 123 andEq assert
0 1 0 andEq assert
0 0 0 andEq assert

1 1 (1) andEq assert
0 1 (0) andEq assert
0 0 (0 assert) andEq assert

(|| ==) orEq =>
// Or
1 1 123 orEq assert
1 0 123 orEq assert
1 1 0 orEq assert
0 0 0 orEq assert

1 1 (0 assert) orEq assert
1 0 (1) orEq assert
0 0 (0) orEq assert

// Not
189 ! ! assert
0 ! assert


